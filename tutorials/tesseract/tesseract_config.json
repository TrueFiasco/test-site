    "navigation": {
      "enableWheelNavigation": true,
      "enableSwipeNavigation": true,
      "enableKeyboardNavigation": true,
      "enableProgressBarNavigation": true,
      "transitions": {
        "duration": 800,
        "easing": "cubic-bezier(0.25, 0.46, 0.45, 0.94)"
      }
    },
    "mobile": {
      "swipeGestures": {
        "enabled": true,
        "minDistance": 50,
        "maxVerticalDrift": 30,
        "minVelocity": 0.3,
        "minDuration": 50,
        "maxDuration": 800,
        "debounceTime": 300
      },
      "motionControl": {
        "enabled": true,
        "sensitivity": 0.008,
        "deadzone": 1,
        "maxRotation": 2,
        "smoothingFactor": 0.15,
        "directMapping": true,
        "gravityBias": 0.1
      }
    },
    "features": {
      "hotspots": {
        "enabled": true,
        "desktopOnly": true,
        "multipleDialogs": true,
        "stickyMode": true
      },
      "widgets": {
        "enabled": true,
        "types": ["tsv-table", "code-viewer", "image"],
        "fullscreenMode": true,
        "copyToClipboard": true,
        "downloadLinks": true
      },
      "accessibility": {
        "keyboardNavigation": true,
        "screenReaderSupport": true,
        "highContrast": false
      }
    },
