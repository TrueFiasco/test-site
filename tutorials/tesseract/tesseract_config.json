{
  "tutorial": {
    "id": "tesseract-tutorial",
    "title": "TESSERACT Tutorial",
    "description": "Interactive TouchDesigner Guide for 4D Hypercube Visualization",
    "version": "1.0",
    "contentSource": {
      "type": "markdown",
      "file": "content.md"
    },
    "hero": {
      "shader": {
        "type": "tesseract",
        "file": "shaderTesseract.js",
        "canvasId": "hypercube-canvas",
        "parameters": {
          "fov": { "default": 7.0, "min": 2, "max": 120, "step": 1 },
          "perspective": { "default": 2.3, "min": 0.1, "max": 10, "step": 0.1 },
          "cameraZ": { "default": 10.0, "min": -100, "max": 50, "step": 1 },
          "lineWidth": { "default": 0.02, "min": 0.001, "max": 0.1, "step": 0.001 }
        }
      },
      "title": "TESSERACT",
      "subtitle": [
        "TouchDesigner Tutorial using GLSL",
        "Interactive 4D Hypercube Visualization"
      ],
      "controls": {
        "desktop": {
          "hint": "Click anywhere to start tutorial",
          "interactions": ["mouse", "scroll", "click"]
        },
        "mobile": {
          "button": "Start Tutorial",
          "interactions": ["touch", "motion", "swipe"]
        }
      }
    },
